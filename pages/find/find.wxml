<!--pages/find/find.wxml-->
<!-- 头部图片 -->
<view class="top_img">
  <image src="../../image/3发现/banner@2x.png"></image>
</view>
<view class="nav">
  <view class="oneof" wx:for='{{nav}}' wx:key='{{index}}' catchtap="getnav" data-number="{{index}}">
    <image src="{{item.img}}"></image>
    <view>{{item.title}}</view>
  </view>
</view>
<!-- 文章列表 -->
<view class="Articlelist">
  <view>文章列表</view>
  <image src="../../image/1首页/youjiantou2@2x.png"></image>
</view>
<view class="list" wx:for='{{Articlelist}}' wx:key='{{index}}' catchtap="getArticle">
  <image src="{{item.img}}"></image>
  <view class="list_box">
    <view class="title">{{item.title}}</view>
    <view class="txt">{{item.txt}}</view>
  </view>
</view>
